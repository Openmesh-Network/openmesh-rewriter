{
  "rewrites": [
    {
      "source": "/openrd/",
      "destination": "https://deeplink-frontend-2-prod.vercel.app/"
    },
    {
      "source": "/openrd/:match*",
      "destination": "https://deeplink-frontend-2-prod.vercel.app/:match*"
    },
    {
      "source": "/oen/",
      "destination": "https://www.weirdband.io/"
    },
    {
      "source": "/oen/:match*",
      "destination": "https://www.weirdband.io/:match*"
    },
    { "source": "/join/", "destination": "https://openvision.info/" },
    {
      "source": "/join/:match*",
      "destination": "https://openvision.info/:match*"
    },
    {
      "source": "/speakers-reg/",
      "destination": "https://speakers-reg.vercel.app/"
    },
    {
      "source": "/speakers-reg/:match*",
      "destination": "https://speakers-reg.vercel.app/:match*"
    },
    { "source": "/", "destination": "https://www.openmesh.network/" },
    {
      "source": "/:match*",
      "destination": "https://www.openmesh.network/:match*"
    }
  ]
}
